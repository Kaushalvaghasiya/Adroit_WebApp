@section Scripts
{

    <script>

        $(function (e) {

            $('.dv-list').hide();
            $('.dv-detail').show();

            GetInvoiceList();

        });

        function addClick() {

            $('.dv-list').slideUp();
            $('.dv-detail').slideDown();

        }

        function GetInvoiceList() {
            try {
                blockUI('container');
                var retrundata = AjaxCall("Get", "@(Url.Content("~/Account/InvoiceList"))", "",
                    function (result) {
                        unblockUI('container');
                        try {
                            if (result.result == RESULT_SUCCESS) {

                                selectedInvoiceDetails = [];

                                result.data.forEach(function (InvoiceDetail) {

                                    selectedInvoiceDetails.push({
                                        id: InvoiceDetail.id,
                                        billNumber: InvoiceDetail.billNumber,
                                        billDate: moment(InvoiceDetail.billDate.split('T')[0]).format('DD/MM/YYYY'),
                                        billAmount: parseFloat(InvoiceDetail.billAmount || 0).toFixed(2),
                                        creditDays: InvoiceDetail.creditDays,
                                        chalanDateTo: moment(InvoiceDetail.chalanDateTo.split('T')[0], 'YYYY-MM-DD').add(1, 'days').format('DD/MM/YYYY'),
                                        paidAmount: parseFloat(InvoiceDetail.paidAmount || 0).toFixed(2),
                                        unPaidAmount: parseFloat(InvoiceDetail.unPaidAmount || 0).toFixed(2)
                                    });
                                });

                                addRow();

                            } else {
                                showError('Error', result.data); //warning,  success, error, info
                            }
                        } catch (e) {
                            unblockUI('container');
                            showError('Error', e); //warning,  success, error, info
                        }
                    },
                    function (error) {

                    });
            }
            catch (err) {
            }
        }

        function addRow() {

            var htmlContentArray = [];
            $('#contentRow').html('');

            for (var i = 0; i < selectedInvoiceDetails.length; i++) {

                var htmlContent = '<tr class="rounded content-rows" id="row-' + i + '">' +
                    '<td class="text-center">' +
                    '<a href="" data-toggle="modal" data-target="#billdetails">' + selectedInvoiceDetails[i].billNumber + '</a>' +
                    '</td>' +
                    '<td class="text-center">' +
                    '<label for="billDate" style="color: #212529;">' + (selectedInvoiceDetails[i].billDate || '') + '</label>' +
