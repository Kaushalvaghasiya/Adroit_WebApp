    function datatablelist() {
      let columns = [];
      let colDef = [];
              colDef.push({
                 "render": function (data, type, row) {
                     return row.title;
                 },
                 "targets": colDef.length,
                 "orderable": false
             });
             colDef.push({
                 "render": function (data, type, row) {
                     return row.customerName;
                 },
                 "targets": colDef.length,
                 "orderable": false
             });
             colDef.push({
                 "render": function (data, type, row) {
                     return row.ownerName;
                 },
                 "targets": colDef.length,
                 "orderable": false
             });
             colDef.push({
                 "render": function (data, type, row) {
                     return row.firmType;
                 },
                 "targets": colDef.length,
                 "orderable": false
             });
             colDef.push({
                 "render": function (data, type, row) {
                     return row.branchLimit;
                 },
                 "targets": colDef.length,
                 "orderable": false
             });
              colDef.push({
                 "render": function (data, type, row) {
                     let btnAct = "<a onclick='getDetail(" + row.id + ");' href='javascript:void(0);' class='btn btn-outline-warning small-icon-btn' data-toggle='tooltip' title='' data-placement='bottom' data-original-title='Edit'><i class='fa fa-pencil'></i></a>";
                     btnAct += " &nbsp;&nbsp; <a href='#' class='btn btn-outline-info small-icon-btn' data-toggle='tooltip' title='' data-placement='bottom' data-original-title='View'><i class='fa fa-info'></i></a>";
                     btnAct += " &nbsp;&nbsp; <a onclick='customerDelete(" + row.id + ");' href='javascript:void(0);' class='btn btn-outline-danger small-icon-btn' data-toggle='tooltip' title='' data-placement='bottom' data-original-title='Delete'><i class='fa fa-trash-o'></i></a>";
                     return btnAct;
                 },
                 "targets": colDef.length,
                 "className": "text-center",
                 "orderable": false
             });

             columns.push({ "data": "title", "name": "title" });
             columns.push({ "data": "customerName", "name": "customerName" });
             columns.push({ "data": "ownerName", "name": "ownerName" });
             columns.push({ "data": "firmType", "name": "firmType" });
             columns.push({ "data": "branchLimit", "name": "branchLimit" });
             columns.push({ "data": "Id", "name": "Id" });

             var table = $('#tblMain').DataTable({
                 ordering: false,
                 searching: true,
                 bInfo: true,
                 bLengthChange: false,
                 info: false,
                 oLanguage: { 'sZeroRecords': 'sEmptyTable' },
                 pageLength: 10,
                 filter: true,
                 paging: true,
                 order: [[1, 'asc']],
                 pagingType: 'full_numbers',
                 orderClasses: false,
                 scrollCollapse: true,
                 bProcessing: true,
                 bServerSide: true,
                 stateSave: false,
                 columnDefs: colDef,
                 columns: columns,
                 bPaginate: true,
                 ajax: {
                     url: '@(Url.Content("~/Admin/CustomerFirmList"))',
                     type: 'GET',
                     data: function (d) { }
                 },

             });
     }

    function fromReset()
    {
             $("#Id").val(0);
             $("#txtCustomerName").val("");
             $("#txtAddressLine1").val("");
             $("#txtAddressLine2").val("");
             $("#txtAddressLine3").val("");
             $("#dropState").select2().val('').trigger("change");
             $("#dropCity").select2().val('').trigger("change");
             $("#dropBusiness").select2().val('').trigger("change");
             $("#txtType").val('');
             $("#txtBusinessName").val("");
             $("#txtPinCode").val("");
             $("#txtContactPersonName").val("");
             $("#txtContactMobile").val("");
             $("#txtAlternateMobile").val("");
             $("#txtPhone").val("");
             $("#txtEmail").val("");
             $("#txtRequirements").val("");
             $("#txtTotalFirms").val("");
             $("#txtAadhaarId").val("");
             $("#txtTotalUsers").val("");
    }

     function saveDetail() {
        try {

                 if($("#txtCustomerName").val().length<=0 ||$("#txtCustomerName").val()==""){
                        showWarning("", "Please enter name.");
                        $("#txtCustomerName").focus();
                          $('.dv-list').slideUp();
                        return false;
                 }
                 if($("#txtAddressLine1").val().length<=0 ||$("#txtAddressLine1").val()==""){
                        showWarning("", "Please enter address.");
                        $("#txtAddressLine1").focus();
                          $('.dv-list').slideUp();
                        return false;
                 }
                  if($("#dropState").val()==""){
                        showWarning("", "Please select state.");
                        $("#dropState").focus();
                          $('.dv-list').slideUp();
                        return false;
                 }
                  if($("#dropCity").val()==""){
                        showWarning("", "Please select city.");
                        $("#dropCity").focus();
                          $('.dv-list').slideUp();
                        return false;
                 }
                 if($("#txtContactPersonName").val().length<=0 ||$("#txtContactPersonName").val()==""){
                        showWarning("", "Please enter contact person name.");
                        $("#txtContactPersonName").focus();
                          $('.dv-list').slideUp();
                        return false;
                 }
                 if($("#txtContactMobile").val().length<=0 ||$("#txtContactMobile").val()==""){
                        showWarning("", "Please enter contact mobile.");
                        $("#txtContactMobile").focus();
                          $('.dv-list').slideUp();
                        return false;
                 }
                  if($("#txtEmail").val().length<=0 ||$("#txtEmail").val()==""){
                        showWarning("", "Please enter email.");
                        $("#txtEmail").focus();
                          $('.dv-list').slideUp();
                        return false;
                 }
                 if($("#dropBusiness").val()==""){
                        showWarning("", "Please enter business.");
                        $("#dropBusiness").focus();
                        return false;
                 }
                 if($("#txtType").val()==""){
                        showWarning("", "Please select customer type.");
                        $("#dropBusiness").focus();
                        return false;
                 }
                 if($("#txtTotalUsers").val().length<=0 ||$("#txtTotalUsers").val()==""){
                        showWarning("", "Please enter total user.");
                        $("#txtTotalUsers").focus();
                        return false;
                 }
                 if($("#txtBusinessName").val().length<=0 ||$("#txtBusinessName").val()==""){
                        showWarning("", "Please enter Business Name.");
                        $("#txtTotalUsers").focus();
                        return false;
                 }

                let isValidemail = IsEmail($("#txtEmail").val());
                if (!isValidemail) {
                    showWarning("", "Please enter valid email");
                    $("#txtEmail").focus();
                    return;
                }

                 var savedata = new Object();
                  savedata.Id = parseInt($("#Id").val());
                  savedata.Name=$("#txtCustomerName").val();
                  savedata.BusinessId=$("#dropBusiness").val();
                  savedata.BusinessName=$("#txtBusinessName").val();
                  savedata.Address1=$("#txtAddressLine1").val();
                  savedata.Address2=$("#txtAddressLine2").val();
                  savedata.Address3=$("#txtAddressLine3").val();
                  savedata.CityId=parseInt($("#dropCity").val());
                  savedata.StateId=parseInt($("#dropState").val());
                  savedata.Pincode=$("#txtPinCode").val();
                  savedata.ContactPersonName=$("#txtContactPersonName").val();
                  savedata.Mobile=$("#txtContactMobile").val();
                  savedata.MobileAlternate=$("#txtAlternateMobile").val();
                  savedata.Phone=$("#txtPhone").val();
                  savedata.Email=$("#txtEmail").val();
                  savedata.Requirement=$("#txtRequirements").val();
                  savedata.TotalFirm=parseInt($("#txtTotalFirms").val());
                  savedata.CustomerType=$("#txtType").val();
                  savedata.AdharUID=$("#txtAadhaarId").val();
                  savedata.TotalUsers=parseInt($("#txtTotalUsers").val());
                  savedata.IsActive=$('#chkActive').is(':checked');
                  savedata.AgreeTerms = true;

                 blockUI('container');

                 $.ajax({
                 type: "POST",
                 url: "@(Url.Action("SaveCustomer", "Admin"))",
                 data: JSON.stringify(savedata),
                 contentType: "application/json; charset=utf-8",
                 dataType: "json",
                 success: function (data) {
                         unblockUI('container');
                         if (data.result == RESULT_SUCCESS) {
                             fromReset();
                             showWarning("", 'Customer saved successfully.');
                             $("#Id").val(0);
                             $('#tblMain').dataTable().fnClearTable();
                             $('#tblMain').dataTable().fnDestroy();
                             datatablelist();
                             $("#btnCancel").click()
                         } else {
                             showError("", data.data);
                         }
                     },
                 error: function (error) {
                         unblockUI('container');
                         showError("", error.statusText);
                     }
                 });

             }
                 catch (e) {
                 unblockUI('container');
                 showError("", e.message);
             }

      }

    function customerDelete(id) {
             if (id == 0) {
                 id = $('#Id').val();
             }
             try {
                 blockUI('container');
                 var retrundata = AjaxCall("Delete", "@(Url.Content("~/Admin/DeleteCustomer"))" + "/" + id, "",
                     function (result) {
                         unblockUI('container');
                         var str = "";
                         try {
                             if (result.result == RESULT_SUCCESS) {
                                 $('#tblMain').dataTable().fnClearTable();
                                 $('#tblMain').dataTable().fnDestroy();
                                 datatablelist();
                             } else {
                                 showError('Error', result.data); //warning,  success, error, info
                             }
                         } catch (e) {
                             showError('Error', e); //warning,  success, error, info
                         }
                     },
                     function (error) {
                         unblockUI('container');
                     });
             }
             catch (err) {
                 unblockUI('container');
             }
         }

    function getDetail(id) {
             try {
                  blockUI('container');
                 var retrundata = AjaxCall("Get", "@(Url.Content("~/Admin/GetCustomer"))" + "/" + id, "",
                     function (result) {
                         unblockUI('container');
                         var str = "";
                         try {
                             if (result.result == RESULT_SUCCESS) {
                                          $("#AddNew").click();
                                          $('#Id').val(result.data.id);
                                          $("#dropState").select2().val(result.data.stateId).trigger("change");
                                          $("#txtCustomerName").val(result.data.name);
                                          $("#dropBusiness").select2().val(result.data.businessId).trigger("change");
                                          $("#txtAddressLine1").val(result.data.address1);
                                          $("#txtAddressLine2").val(result.data.address2);
                                          $("#txtAddressLine3").val(result.data.address3);
                                          $("#txtPinCode").val(result.data.pincode);
                                          $("#txtContactPersonName").val(result.data.contactPersonName);
                                          $("#txtContactMobile").val(result.data.mobile);
                                          $("#txtAlternateMobile").val(result.data.mobileAlternate);
                                          $("#txtPhone").val(result.data.phone);
                                          $("#txtEmail").val(result.data.email);
                                          $("#txtRequirements").val(result.data.requirement);
                                          $("#txtTotalFirms").val(result.data.totalFirm);
                                          $('#chkActive').prop('checked', result.data.isActive);
                                          $("#txtAadhaarId").val(result.data.adharUID);
                                          $("#txtTotalUsers").val(result.data.totalUsers);
                                          $("#txtType").val(result.data.customerType);
                                          $("#txtBusinessName").val(result.data.businessName);
                                          $("#txtType option:contains(" + result.data.customerType + ")").attr('selected', 'selected');
                                          $("#hdnCityId").val(result.data.cityId);

                             } else {
                                 showError('Error', result.data); //warning,  success, error, info
                             }
                         } catch (e) {
                             unblockUI('container');
                             showError('Error', e); //warning,  success, error, info
                         }
                     },
                     function (error) {

                     });
             }
             catch (err) {
             }
         }

