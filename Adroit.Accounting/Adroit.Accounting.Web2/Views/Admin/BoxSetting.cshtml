        function bindCustomerBySoftware(softwareId) {
            try {
                $("#CustomerId").empty();

                if (softwareId == '' || softwareId == null) return;

                blockUI('container');
                var retrundata = AjaxCall("GET", "@(Url.Content("~/Common/GetCustomerBySoftware/"))" + softwareId, "",
                    function (data) {
                        unblockUI('container');
                        if (data.result == RESULT_SUCCESS) {
                            if (data.data.length > 0) {
                                for (var i = 0; i < data.data.length; i++) {
                                    $('#CustomerId').append(new Option(data.data[i].text, data.data[i].value, false, false));
                                }
                            }
                            $('#CustomerId').val($("#hdCustomerId").val()).trigger('change');
                        } else {
                            showError("", data.data);
                        }

                        if ($("#hdCustomerId").val() == "0")
                        {
                            getDetailBySoftware(softwareId);
                        }
                    },
                    function (error) {
                        unblockUI('container');
                        showError("", error.statusText);
                    });
            }
            catch (err) {
                unblockUI('container');
                showError("", e.message);
            }
        }

