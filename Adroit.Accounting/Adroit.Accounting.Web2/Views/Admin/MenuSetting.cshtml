                blockUI('container');
                var retrundata = AjaxCall("Get", "@(Url.Content("~/Admin/DeleteMenuSetting"))" + "/" + id, "",
                    function (result) {
                        unblockUI('container');
                        try {
                            if (result.result == RESULT_SUCCESS) {
                                $('#Title option[value="' + $('#Title').val() + '"]').detach();
                                refreshDatatableList();
                                fromReset();
                                $('.btn-back').click();
