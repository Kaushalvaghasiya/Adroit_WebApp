            }
            catch (err) {
                unblockUI('container');
                showError("", e.message);
            }
        }

        function bindTalukaList(districtId) {
            try {
                $("#Taluka").empty();
                if (districtId == '' || districtId == null) return;

                blockUI('container');
                var retrundata = AjaxCall("GET", "@(Url.Content("~/Common/GetTalukas"))" + "?districtId=" + districtId, "",
                    function (data) {
                        unblockUI('container');
                        if (data.result == RESULT_SUCCESS) {
                            if (data.data.length > 0) {
                                for (var i = 0; i < data.data.length; i++) {
                                    $('#Taluka').append(new Option(data.data[i].text, data.data[i].value, false, false));
                                }
                            }
                            $('#Taluka').val($("#hdnTalukaId").val()).trigger('change');
                        } else {
                            showError("", data.data);
                        }
                    },
                    function (error) {
                        unblockUI('container');
                        showError("", error.statusText);
                    });
            }
            catch (err) {
                unblockUI('container');
                showError("", e.message);
            }
        }

        function bindCityList(talukaId) {
            try {
                $("#CityId").empty();
                if (talukaId == '' || talukaId == null) return;

                blockUI('container');
                var retrundata = AjaxCall("GET", "@(Url.Content("~/Common/GetCities"))" + "?talukaId=" + talukaId, "",
                    function (data) {
                        unblockUI('container');
                        if (data.result == RESULT_SUCCESS) {
                            if (data.data.length > 0) {
                                for (var i = 0; i < data.data.length; i++) {
                                    $('#CityId').append(new Option(data.data[i].text, data.data[i].value, false, false));
                                }
                            }
                            $('#CityId').val($("#hdnCityId").val()).trigger('change');
                        } else {
                            showError("", data.data);
                        }
                    },
                    function (error) {
                        unblockUI('container');
                        showError("", error.statusText);
                    });
            }
            catch (err) {
                unblockUI('container');
                showError("", e.message);
            }
        }

        function fromReset() {
            $("#Id").val(0);
            $("#CustomerName").val("");
            $("#BusinessName").val("");
            $("#BusinessId").val('').trigger("change");
            $("#Address1").val("");
            $("#Address2").val("");
            $("#Address3").val("");
            $("#Country").val('').trigger("change");
            $("#StateId").val('').trigger("change");
            $("#District").val('').trigger("change");
            $("#Taluka").val('').trigger("change");
            $("#CityId").val('').trigger("change");
            $("#Business").val('').trigger("change");
            $("#CustomerType").val('').trigger("change");
            $("#StatusId").val('').trigger("change");
            $("#PinCode").val("");
            $("#ContactPersonName").val("");
            $("#Mobile").val("");
            $("#MobileAlternate").val("");
            $("#Phone").val("");
            $("#Email").val("");
            $("#Requirements").val("");
            $("#TotalFirms").val("1");
            $("#AadhaarUID").val("");
            $("#TotalUsers").val("1");
            $("#Active").prop('checked', false);

            $("#hdnCountryId").val("");
            $("#hdnStateId").val("");
            $("#hdnDistrictId").val("");
            $("#hdnTalukaId").val("");
            $("#hdnCityId").val("");

            $("#activeTabName").text("Add");
            $(".btn-delete").hide();
        }

        function saveItem() {
            try {
                if ($("#CustomerName").val().trim() == "") {
                    showWarning("", "Please enter name.");
                    $("#CustomerName").focus();
                    return false;
                }
                if ($("#CustomerName").val().trim().length <= 2) {
                    showWarning("", "Enter valid customer name.");
                    $("#CustomerName").focus();
                    return false;
                }
                if ($("#BusinessName").val().trim() == "") {
                    showWarning("", "Please enter Business Name.");
                    $("#BusinessName").focus();
                    return false;
                }
                if ($("#BusinessName").val().trim().length <= 2) {
                    showWarning("", "Please enter valid business name.");
                    $("#BusinessName").focus();
                    return false;
                }
                if ($("#BusinessId").val() == null || $("#BusinessId").val() == "") {
                    showWarning("", "Please select business.");
                    $("#BusinessId").focus();
                    return false;
                }
                if ($("#Address1").val().trim() == "") {
                    showWarning("", "Please enter address.");
                    $("#Address1").focus();
                    return false;
                }
                if ($("#Country").val() == null || $("#Country").val() == "") {
                    showWarning("", "Please select country.");
                    $("#Country").focus();
                    return false;
                }
                if ($("#StateId").val() == null || $("#StateId").val() == "") {
                    showWarning("", "Please select state.");
                    $("#StateId").focus();
                    return false;
                }
                if ($("#District").val() == null || $("#District").val() == "") {
                    showWarning("", "Please select district.");
                    $("#District").focus();
                    return false;
                }
                if ($("#Taluka").val() == null || $("#Taluka").val() == "") {
                    showWarning("", "Please select taluka.");
                    $("#Taluka").focus();
                    return false;
                }
                if ($("#CityId").val() == null || $("#CityId").val() == "") {
                    showWarning("", "Please select city.");
                    $("#CityId").focus();
                    return false;
                }
                if ($("#ContactPersonName").val().trim() == "") {
                    showWarning("", "Please enter contact person name.");
                    $("#ContactPersonName").focus();
                    return false;
                }
                if ($("#ContactPersonName").val().trim().length <= 2) {
                    showWarning("", "Please enter valid contact person name.");
                    $("#ContactPersonName").focus();
                    return false;
                }
                if ($("#Mobile").val().trim() == "") {
                    showWarning("", "Please enter contact mobile.");
                    $("#Mobile").focus();
                    return false;
                }
                if ($("#Mobile").val().trim().length != 10) {
                    showWarning("", "Please enter valid mobile number.");
                    $("#Mobile").focus();
                    return false;
                }
                if ($("#MobileAlternate").val().trim().length >= 1) {
                    if ($("#MobileAlternate").val().trim().length != 10) {
                        showWarning("", "Please enter valid alternate mobile number.");
                        $("#MobileAlternate").focus();
                        return false;
                    }
                }
                if ($("#Email").val().trim() == "") {
                    showWarning("", "Please enter email.");
                    $("#Email").focus();
                    return false;
                }
                if ($("#AadhaarUID").val().trim().length > 0 && $("#AadhaarUID").val().trim().length != 12) {
                    showWarning("", "Please enter valid Aadhaar UID.");
                    $("#AadhaarUID").focus();
                    return false;
                }
                if ($("#AadhaarUID").val().trim().length > 0 && !isNumber($("#AadhaarUID").val().trim())) {
                    showWarning("", "Please enter valid Aadhaar UID.");
                    $("#AadhaarUID").focus();
                    return false;
                }
                if ($("#CustomerType").val() == "" || $("#CustomerType").val() == null) {
                    showWarning("", "Please select customer type.");
                    $("#CustomerType").focus();
                    return false;
                }
                if (!isNumber($("#Mobile").val().trim())) {
                    showWarning("", "Please enter valid mobile number.");
                    $("#Mobile").focus();
                    return false;
                }
                if (!isNumber($("#MobileAlternate").val().trim())) {
                    showWarning("", "Please enter valid alternate mobile number.");
                    $("#MobileAlternate").focus();
                    return false;
                }
                if (!isEmail($("#Email").val().trim())) {
                    showWarning("", "Please enter valid email");
                    $("#Email").focus();
                    return false;
                }
                if ($("#StatusId").val() == null || $("#StatusId").val() == "") {
                    showWarning("", "Please select status.");
                    $("#StatusId").focus();
                    return false;
                }

                if (!isNumber($("#TotalFirms").val())) {
                    showWarning("", "Please enter valid number of firms.");
                    $("#TotalFirms").focus();
                    return false;
                }
                if (parseInt($("#TotalFirms").val()) <= 0) {
                    $("#TotalFirms").val("1");
                }
                if (!isNumber($("#TotalUsers").val())) {
                    showWarning("", "Please enter valid number of users.");
                    $("#TotalUsers").focus();
                    return false;
                }
                if (parseInt($("#TotalUsers").val()) <= 0) {
                    $("#TotalUsers").val("1");
                }
                var savedata = new Object();
                savedata.Id = parseInt($("#Id").val());
                savedata.Name = $("#CustomerName").val();
                savedata.BusinessId = $("#BusinessId").val();
                savedata.BusinessName = $("#BusinessName").val();
                savedata.Address1 = $("#Address1").val();
                savedata.Address2 = $("#Address2").val();
                savedata.Address3 = $("#Address3").val();
                savedata.CityId = parseInt($("#CityId").val());
                savedata.StateId = parseInt($("#StateId").val());
                savedata.Pincode = $("#PinCode").val();
                savedata.ContactPersonName = $("#ContactPersonName").val();
                savedata.Mobile = $("#Mobile").val();
                savedata.MobileAlternate = $("#MobileAlternate").val();
                savedata.Phone = $("#Phone").val();
                savedata.Email = $("#Email").val();
                savedata.Requirement = $("#Requirements").val();
                savedata.TotalFirm = parseInt($("#TotalFirms").val());
                savedata.CustomerType = $("#CustomerType").val();
                savedata.AdharUID = $("#AadhaarUID").val();
                savedata.TotalUsers = parseInt($("#TotalUsers").val());
                savedata.Active = $('#Active').is(':checked');
                savedata.AgreeTerms = true;
                savedata.StatusId = $("#StatusId").val();

                blockUI('container');

                $.ajax({
                    type: "POST",
                    url: "@(Url.Action("SaveCustomer", "Admin"))",
                    data: JSON.stringify(savedata),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {
                        unblockUI('container');
                        if (data.result == RESULT_SUCCESS) {
                            fromReset();
                            showWarning("", 'Customer saved successfully.');
                            $("#Id").val(0);
                            refreshDatatableList();
                            $('.btn-back').click();
                        } else {
                            showError("", data.data);
                        }
                    },
                    error: function (error) {
                        unblockUI('container');
                        showError("", error.statusText);
                    }
                });

            }
            catch (e) {
                unblockUI('container');
                showError("", e.message);
            }

        }

        function deleteItem(id) {
            if (id == 0) {
                id = $('#Id').val();
            }
            try {
                blockUI('container');
                var retrundata = AjaxCall("Get", "@(Url.Content("~/Admin/DeleteCustomer"))" + "/" + id, "",
                    function (result) {
                        unblockUI('container');
                        var str = "";
                        try {
                            if (result.result == RESULT_SUCCESS) {
                                refreshDatatableList();
                                fromReset();
                                $('.btn-back').click();
                            } else {
                                showError('Error', result.data); //warning,  success, error, info
                            }
                        } catch (e) {
                            showError('Error', e); //warning,  success, error, info
                        }
                    },
                    function (error) {
                        unblockUI('container');
                    });
            }
            catch (err) {
                unblockUI('container');
            }
        }

        function getDetail(id, isViewMode) {
            try {
                blockUI('container');
                var retrundata = AjaxCall("Get", "@(Url.Content("~/Admin/GetCustomer"))" + "/" + id, "",
                    function (result) {
                        unblockUI('container');
                        var str = "";
                        try {
                            if (result.result == RESULT_SUCCESS) {
                                addClick();
                                if (isViewMode) {
                                    setScreenMode(true);
                                    $("#activeTabName").text("VIEW ID# " + result.data.id);
                                } else {
                                    setScreenMode(false);
                                    $("#activeTabName").text("EDIT (ID# " + result.data.id + ")");
                                }

                                $('#Id').val(result.data.id);
                                $("#CustomerName").val(result.data.name);
                                $("#BusinessName").val(result.data.businessName);
                                $("#BusinessId").val(result.data.businessId).trigger("change");
                                $("#Address1").val(result.data.address1);
                                $("#Address2").val(result.data.address2);
                                $("#Address3").val(result.data.address3);
                                $("#PinCode").val(result.data.pincode);
                                $("#ContactPersonName").val(result.data.contactPersonName);
                                $("#Mobile").val(result.data.mobile);
                                $("#MobileAlternate").val(result.data.mobileAlternate);
                                $("#Phone").val(result.data.phone);
                                $("#Email").val(result.data.email);
                                $("#Requirements").val(result.data.requirement);
                                $("#AadhaarUID").val(result.data.adharUID);
                                $("#TotalFirms").val(result.data.totalFirm);
                                $("#TotalUsers").val(result.data.totalUsers);
                                $('#Active').prop('checked', result.data.active);
                                $("#CustomerType").val(result.data.customerTypeId).trigger("change");
                                $("#StatusId").val(result.data.statusTypeId).trigger("change");

                                $("#hdnCountryId").val(result.data.countryId);
                                $("#hdnStateId").val(result.data.stateId);
                                $("#hdnDistrictId").val(result.data.districtId);
                                $("#hdnTalukaId").val(result.data.talukaId);
                                $("#hdnCityId").val(result.data.cityId);
                                $("#Country").val(result.data.countryId).trigger("change");
                            } else {
                                showError('Error', result.data); //warning,  success, error, info
                            }
                        } catch (e) {
                            unblockUI('container');
                            showError('Error', e); //warning,  success, error, info
                        }
                    },
                    function (error) {

                    });
            }
            catch (err) {
            }
        }
    </script>
