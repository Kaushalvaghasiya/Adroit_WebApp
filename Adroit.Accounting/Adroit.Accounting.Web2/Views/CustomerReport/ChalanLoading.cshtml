@section Scripts
    {

    <script>
        $(function (e) {

            $('.dv-parameters').show();
            $('.dv-report').hide();

            var table = $('#tblMain').DataTable({
                "bAutoWidth": false,
                lengthChange: false,
                ordering: true,
                searching: true,
                paging: true,
                pageLength: 50,
                order: [[0, 'asc']],
                pagingType: 'full_numbers',
                orderClasses: false,
                // columnDefs: colDef,
                // columns: columns,
                language: {
                    paginate: {
                        next: '<i class="fa fa-angle-right"></i>', // or '→'
                        previous: '<i class="fa fa-angle-left"></i>', // or '←'
                        first: '<i class="fa fa-angle-double-left"></i>', // or '→'
                        last: '<i class="fa fa-angle-double-right"></i>' // or '←'
                    }
                },
                buttons: ['copy', 'excel', 'pdf', 'colvis'],
                //processing: true,
                //serverSide: true,
                // ajax: {
                //     url: '@(Url.Content("~/CustomerReport/LRRegister"))',
                //     type: 'GET',
                //     data: function (d) {

                //     }
                // },
                // "initComplete": function (settings, json) {
                //     gridInitComplete(table);
                // },
                // "drawCallback": function (settings) {
                //     gridDrawCallback(this);
                // }
            });

            table.buttons().container().appendTo('#tblMain_wrapper .col-md-6:eq(0)');

            $(".show-report").on("click", function () {
                $('.dv-parameters').slideUp();
                $('.dv-report').slideDown();
            });

            $(".btn-back").on("click", function () {
                $('.dv-report').slideUp();
                $('.dv-parameters').slideDown();
            });

        });

        function refreshDatatableList() {
            $('#tblMain').DataTable().ajax.reload();
        }

    </script>
}
