        function getLRRateDetail() {
            try {

                var cityIdTo = $("#CityIdTo").val();
                var billPartyId = $("#BillAccountBranchMappingId").val();
                var rateOnId = $("#LRRateOnId").val();

                if ((cityIdTo == '' || cityIdTo == null)
                    || (billPartyId == '' || billPartyId == null)
                    || (rateOnId == '' || rateOnId == null)) return;

                var retrundata = AjaxCall("Get", "@(Url.Content("~/Customer/GetRate/"))" + cityIdTo + "/" + billPartyId + "/" + rateOnId, "",
                    function (data) {
                        if (data.result == RESULT_SUCCESS) {
                            if (data.data != null) {
                                $('#Rate').val(data.data.rate);
                                $('#Rate').attr('disabled', data.data.isRateDisable == 1 ? true : false);
                                //calcFreight();
                                calcNetAmount();
                            } else $('#Rate').attr('disabled', false);
                        } else {
                            showError("", data.data);
                        }
                    },
                    function (error) {
                        showError("", error.statusText);
                    });
            }
            catch (err) {
                showError("", e.message);
            }
        }

        function calcFreight(selectedLRRate) {
            var selectedLRRate = $("#LRRateOnId option:selected").text();
            var rate = +getFloatValueZero("Rate", 2);
            var parcel = +getFloatValueZero("Parcel", 2);
            var chargeWeight = +getFloatValueZero("ChargeWeight", 2);

            if (selectedLRRate == 'Parcel') {
                $("#Freight").val((rate * parcel).toFixed(2));
            }
            else if (selectedLRRate == 'Weight') {
                $("#Freight").val((rate * chargeWeight).toFixed(2));
            }
            else {
                $("#Freight").val(rate);
            }
        }

        function calcNetAmount() {
            calcFreight();

            var Charges1 = +getFloatValueZero("Charges1", 2);
            var Charges2 = +getFloatValueZero("Charges2", 2);
            var Charges3 = +getFloatValueZero("Charges3", 2);
            var Charges4 = +getFloatValueZero("Charges4", 2);
            var Charges5 = +getFloatValueZero("Charges5", 2);
            var Charges6 = +getFloatValueZero("Charges6", 2);
            var Freight = +getFloatValueZero("Freight", 2);
            var netAmount = (Charges1 + Charges2 + Charges3 + Charges4 + Charges5 + Charges6 + Freight);

            $("#NetAmount").val(netAmount.toFixed(2));

        }

        function updatePrivateMarka() {
            var invoiceNoValue = $("#InvoiceNo").val();
            var parcelValue = $("#Parcel").val();

            if (invoiceNoValue) {
                $("#PrivateMarka").val(invoiceNoValue + '*' + parcelValue);
            } else {
                $("#PrivateMarka").val(parcelValue);
            }
        }

