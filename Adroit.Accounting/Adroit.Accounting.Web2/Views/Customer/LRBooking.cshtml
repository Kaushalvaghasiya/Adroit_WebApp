                "targets": colDef.length,
                "className": "text-center",
            });
            colDef.push({
                "render": function (data, type, row) {
                    return row.consignor;
                },
                "targets": colDef.length,
            });
            colDef.push({
                "render": function (data, type, row) {
                    return row.netAmount.toFixed(2);
