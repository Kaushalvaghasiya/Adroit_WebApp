                                                            if (FirstRow != null)
                                                            {
                                                                var FirstRowId = FirstRow.Value;
                                                                <option value="@FirstRowId" selected>@FirstRow.Text</option>
                                                            }

                                                            foreach (var o in Model.LRDeliveryTypeList.Where(x => x.Text != "-"))
                                                            {
                                                                <option value="@o.Value">@o.Text</option>
                                                            }
                                                        }
