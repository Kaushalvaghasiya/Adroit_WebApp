@section Scripts
{
    <script>

        $(function (e) {

            $('.dv-list').show();
            $('.dv-detail').hide();

            bindDatatableList();

            $("#CityIdTo").on('change', function (e) {
                $('#Rate').attr('disabled', false);
                getLRRateDetail($(this).val(), $("#BillAccountBranchMappingId").val(), $("#LRRateOnId").val());
            });

            $('#EWayBill-search').on('click', function () {
                var Data = "Your dynamically generated content goes here.";
                $(this).attr('data-content', Data);
            });

            $("#BillAccountBranchMappingId").on('change', function (e) {
                $('#Rate').attr('disabled', false);
                getLRRateDetail($("#CityIdTo").val(), $(this).val(), $("#LRRateOnId").val());
            });

            $("#Parcel").on('keyup', function (e) {
                updatePrivateMarka();
                CalcFreight($("#hdnLRRateOn").val());
            });

            $("#LRRateOnId").on('change', function (e) {
                $('#Rate').attr('disabled', false);
                if ($("#LRRateOnId").val() != '' || $("#LRRateOnId").val() != null) {
                    var LRRateElement = document.getElementById("select2-LRRateOnId-container");
                    $("#hdnLRRateOn").val(LRRateElement.getAttribute("title"));
                    getLRRateDetail($("#CityIdTo").val(), $("#BillAccountBranchMappingId").val(), $(this).val());
                }
            });

            var roundOff = @((Model.CustomerFirmTransportSetting.IsWeightRoundOff ? "true" : "false").ToLower());
            if (roundOff == true) {
                $("#ChargeWeight").on("change", function (e) {
                    var chargeWeight = parseFloat(getFloatValueZero("ChargeWeight", 3));

                    if (chargeWeight >= 1 && chargeWeight <= 49.999) {
                        var roundedWeight = Math.ceil(chargeWeight / 50) * 50;
                        $("#ChargeWeight").val(roundedWeight.toFixed(3));
                    }

                    if (chargeWeight >= 50.001) {
                        var roundedWeight = Math.ceil(chargeWeight / 5) * 5;
                        $("#ChargeWeight").val(roundedWeight.toFixed(3));
                    }

                    if (chargeWeight == 50 || chargeWeight == 0) {
                        $("#ChargeWeight").val(chargeWeight.toFixed(3));
                    }

                });

                $("#ChargeWeight").on("focus", function (e) {
                    if ($("#ChargeWeight").val() == '' || $("#ChargeWeight").val() == null) {
                        $("#ChargeWeight").val(50.000);
                    }
                });

                $("#ActualWeight").on("change", function (e) {
                    var actualWeight = parseFloat(getFloatValueZero("ActualWeight", 3));
                    $("#ActualWeight").val(actualWeight.toFixed(3));

                    if (actualWeight >= 1 && actualWeight <= 49.999) {
                        var roundedWeight = Math.ceil(actualWeight / 50) * 50;
                        $("#ChargeWeight").val(roundedWeight.toFixed(3));
                    }

                    if (actualWeight >= 50.001) {
                        var roundedWeight = Math.ceil(actualWeight / 5) * 5;
                        $("#ChargeWeight").val(roundedWeight.toFixed(3));
                    }

                    if (actualWeight == 50 || actualWeight == 0) {
                        $("#ChargeWeight").val(actualWeight.toFixed(3));
                    }
                });
            }
            else {
                $("#ActualWeight").on("focusout", function (e) {
                    var actualWeight = getFloatValueZero("ActualWeight", 3);
                    $("#ActualWeight").val(actualWeight.toFixed(3));
                    $("#ChargeWeight").val(actualWeight.toFixed(3));
                });

                $("#ChargeWeight").on("focusout", function (e) {
                    var chargeWeight = getFloatValueZero("ChargeWeight", 3);
                    $("#ChargeWeight").val(chargeWeight.toFixed(3));
                });
            }

            $("#InvoiceNo").on('keyup', function (e) {
                updatePrivateMarka();
            });

            $("#Charges1, #Charges2, #Charges3, #Charges4, #Charges5, #Charges6, #Freight").on('change', function (e) {
                var fieldId = $(this).attr('id');
                var currentValue = parseFloat($("#" + fieldId).val());
                var formattedValue = currentValue.toFixed(2);
                $("#" + fieldId).val(formattedValue);

                CalcNetAmount();
            });

            $("#ChargeWeight").on('change', function (e) {
                CalcFreight($("#hdnLRRateOn").val());
            });

            $("#ActualWeight").on('change', function (e) {
                CalcFreight($("#hdnLRRateOn").val());
            });

            $("#Rate").on('change', function (e) {
                var fieldId = $(this).attr('id');
                var currentValue = parseFloat($("#" + fieldId).val());
                var formattedValue = currentValue.toFixed(2);
                $("#" + fieldId).val(formattedValue);

                CalcFreight($("#hdnLRRateOn").val());
            });

            $("#AccountBranchMappingId, #DeliveryAccountBranchMappingId").on('change', function (e) {
                BindBillParty();
            });

            $("#LRPayTypeId").on('change', function (e) {
                BindBillParty(true);
            });

            addClick();
            var cookieValue = getCookie('@Html.Raw(lastLrToCityCookieName)');
            $("#CityIdTo").val(cookieValue).trigger('change');
        });

        function BindBillParty(isForceFullyChangeBillParty = false) {
            if ($("#BillAccountBranchMappingId").val() == '' || $("#BillAccountBranchMappingId").val() == null || isForceFullyChangeBillParty) {
                if ($("#LRPayTypeId").val() != '' || $("#LRPayTypeId").val() != null) {
                    var spanElement = document.getElementById("select2-LRPayTypeId-container");
                    var selectedLRPayType = spanElement.getAttribute("title");
                    if (selectedLRPayType != '' || selectedLRPayType != null) {
                        if (selectedLRPayType == 'To Pay' || selectedLRPayType == 'FOC') {
                            if ($("#DeliveryAccountBranchMappingId").val() != '' || $("#DeliveryAccountBranchMappingId").val() != null) {
                                $("#BillAccountBranchMappingId").val($("#DeliveryAccountBranchMappingId").val()).trigger('change');
                            }
                        }
                        if (selectedLRPayType == 'Paid' || selectedLRPayType == 'TBB') {
                            if ($("#AccountBranchMappingId").val() != '' || $("#AccountBranchMappingId").val() != null) {
                                $("#BillAccountBranchMappingId").val($("#AccountBranchMappingId").val()).trigger('change');
                            }
                        }
                    }
                }
            }
        }

