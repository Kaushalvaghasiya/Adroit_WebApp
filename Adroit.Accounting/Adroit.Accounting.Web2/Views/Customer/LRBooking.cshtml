                    if (!(row.deleted || row.isLRBookingPurchased)) {
                        btnAct += "<a href='#' class='btn-cancel-grid btn btn-outline-danger small-icon-btn' data-value='" + row.id + "' data-toggle='tooltip' title='Cancel' data-placement='bottom' data-original-title='Cancel'><i class='fa fa-remove'></i></a>";
                    }
                    btnAct += "</div>";
                    return btnAct;
