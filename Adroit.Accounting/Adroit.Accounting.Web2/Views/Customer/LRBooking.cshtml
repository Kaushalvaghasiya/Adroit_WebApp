                                            <div class="col-md-4">
                                                <input type="text" class="form-control" id="Truck" placeholder="" readonly tabindex="-1">
                                            </div>
                                        </div>
                                        <div class="row mb-1">
                                            <label for="Ch" class="col-md-2 form-label">Invoice#</label>
                                            <div class="col-md-4">
                                                <input type="text" class="form-control" id="InvoiceNo" placeholder="" readonly tabindex="-1">
                                            </div>
                                            <label for="Truck" class="col-md-2 form-label">Invoice Date</label>
                                            <div class="col-md-4">
                                                <input type="text" class="form-control" id="InvoiceDate" placeholder="" readonly tabindex="-1">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
