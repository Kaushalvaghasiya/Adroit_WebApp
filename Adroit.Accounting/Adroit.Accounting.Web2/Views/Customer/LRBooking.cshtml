        var pagedatakey= 'lrpagedata';
        function savePageData() {
            try {
                var data = {};
                $(".account-save-cookie").each(function () {
                    data[$(this).attr("id")] = $(this).val();
                });

                $.cookie(pagedatakey, JSON.stringify(data));
            } catch (e) {

            }
        };

        // Reload all elements from a cookie
        function loadPageData() {
            try {
                var data = JSON.parse($.cookie(pagedatakey));
                if (data != null) {
                    for (var id in data) {
                        if ($("#" + id).is('select')) {
                            $("#" + id).val(data[id]).trigger('change');
                        }
                        else{
                            $("#" + id).val(data[id]);
                        }
                    }

                    $.removeCookie(pagedatakey);
                }
            } catch (e) {

            }
        };
    </script>
