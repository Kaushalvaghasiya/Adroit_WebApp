                    if (!/^[-+]?(\d{1,16}(\.\d{0,2})?|\.\d{1,2})$/.test($("#Freight").val())) {
                        showWarning("", "Freight must be upto 16 characters before decimal point and 2 characters after decimal point long.");
                        $("#Freight").focus();
                        return false;
                    }
                }

