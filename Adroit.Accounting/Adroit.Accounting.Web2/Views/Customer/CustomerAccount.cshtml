                                    $("#Address2").val(result.data.address2);
                                    $("#Address3").val(result.data.address3);
                                    $("#Pincode").val(result.data.pincode);
                                    $("#PAN").val(result.data.pan);
                                    
                                    $("#CityId").val(result.data.cityId).trigger("change");
