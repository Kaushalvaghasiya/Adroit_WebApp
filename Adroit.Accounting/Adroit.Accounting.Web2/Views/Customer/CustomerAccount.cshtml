                    function (result) {
                        unblockUI('container');
                        try {
                            if (result.result == RESULT_SUCCESS) {

                                if (result.data != null) {
                                    $("#GSTNumber").val(result.data.gstNumber);

                                    if ($("#Name option[value='" + result.data.name + "']").length === 0) {
                                        $("#Name").append($("<option>", {
                                            value: result.data.name,
                                            text: result.data.name
                                        }));
