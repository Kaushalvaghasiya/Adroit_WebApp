            $("#DenierWeight").append(new Option(maxvalue.toFixed(3), maxvalue.toFixed(3), true, true)).trigger('change');
            $("#RatePerMeter").append(new Option(maxvalue.toFixed(2), maxvalue.toFixed(2), true, true)).trigger('change');

            $("#RateUpdate").prop('checked', true);
            $("#Active").prop('checked', true);

            $("#activeTabName").text("Add");
            $(".btn-delete").hide();
        }

        function saveItem() {
            try {

                if ($("#Title").val() == "") {
                    showWarning("", "Please enter title");
                    $("#Title").focus();
                    return false;
                }

                if (($("#StockTypeId").val() == null) || ($("#StockTypeId").val() == "")) {
                    showWarning("", "Please select Stock Type");
                    $("#StockTypeId").focus();
                    return false;
                }

                if (($("#UQCId").val() == null) || ($("#UQCId").val() == "")) {
                    showWarning("", "Please select UOM.");
                    $("#UQCId").focus();
                    return false;
                }

                var UQC = $("#UQCId option:selected").text();
                if (!UQC.includes('-')) {
                    showWarning("", "Please select UOM in Code-CodeEWayBill format.");
                    $("#UQCId").focus();
                    return false;
                }

                if ($("#RateRetail").val() != null && $("#RateRetail").val() != "") {
                    if (!/^[-+]?(\d{1,2}(\.\d{0,3})?|\.\d{1,3})$/.test($("#RateRetail").val())) {
                        showWarning("", "Retail Rate must be upto 2 characters before decimal point and 3 characters after decimal point long.");
                        $("#RateRetail").focus();
                        return false;
                    }
                }

                if ($("#Mrp").val() != null && $("#Mrp").val() != "") {
                    if (!/^[-+]?(\d{1,2}(\.\d{0,3})?|\.\d{1,3})$/.test($("#Mrp").val())) {
                        showWarning("", "Mrp must be upto 2 characters before decimal point and 3 characters after decimal point long.");
                        $("#Mrp").focus();
                        return false;
                    }
                }

                if ($("#DistributorRate").val() != null && $("#DistributorRate").val() != "") {
                    if (!/^[-+]?(\d{1,2}(\.\d{0,3})?|\.\d{1,3})$/.test($("#DistributorRate").val())) {
                        showWarning("", "Distributor Rate must be upto 2 characters before decimal point and 3 characters after decimal point long.");
                        $("#DistributorRate").focus();
                        return false;
                    }
                }

                if ($("#DealerRate").val() != null && $("#DealerRate").val() != "") {
                    if (!/^[-+]?(\d{1,2}(\.\d{0,3})?|\.\d{1,3})$/.test($("#DealerRate").val())) {
                        showWarning("", "Dealer Rate must be upto 2 characters before decimal point and 3 characters after decimal point long.");
                        $("#DealerRate").focus();
                        return false;
                    }
                }

                if ($("#AveragePack").val() != null && $("#AveragePack").val() != "") {
                    if (!/^[-+]?(\d{1,3}(\.\d{0,2})?|\.\d{1,2})$/.test($("#AveragePack").val())) {
                        showWarning("", "Average Pack must be upto 3 characters before decimal point and 2 characters after decimal point long.");
                        $("#AveragePack").focus();
                        return false;
                    }
                }

                if ($("#BoxPack").val() != null && $("#BoxPack").val() != "") {
                    if (!/^[-+]?(\d{1,3}(\.\d{0,2})?|\.\d{1,2})$/.test($("#BoxPack").val())) {
                        showWarning("", "Box Pack must be upto 3 characters before decimal point and 2 characters after decimal point long.");
                        $("#BoxPack").focus();
                        return false;
                    }
                }

                if (($("#GSTCalculationId").val() == null) || ($("#GSTCalculationId").val() == "")) {
                    showWarning("", "Please select GST Calculation.");
                    $("#GSTCalculationId").focus();
                    return false;
                }

                if (($("#GSTRateId").val() == null) || ($("#GSTRateId").val() == "")) {
                    showWarning("", "Please select IGST Or GST.");
                    $("#GSTRateId").focus();
                    return false;
                }

                if ($("#GstCentralCess").val() == null || $("#GstCentralCess").val() == "" || $("#GstCentralCess").val().length > 5) {
                    showWarning("", "Central Cess must be upto 5 characters long.");
                    $("#GstCentralCess").focus();
                    return false;
                }

                if (!/^[-+]?(\d{1,3}(\.\d{0,2})?|\.\d{1,2})$/.test($("#GstStateCess").val())) {
                    showWarning("", "State Cess must be upto 3 characters before decimal point and 2 characters after decimal point long.");
                    $("#GstStateCess").focus();
                    return false;
                }

                if (!/^[-+]?(\d{1,2}(\.\d{0,2})?|\.\d{1,2})$/.test($("#Discount").val())) {
                    showWarning("", "Discount must be upto 2 characters before decimal point and 2 characters after decimal point long.");
                    $("#Discount").focus();
                    return false;
                }

                if ($("#PurchaseRate").val() != null && $("#PurchaseRate").val() != "") {
                    if (!/^[-+]?(\d{1,2}(\.\d{0,3})?|\.\d{1,3})$/.test($("#PurchaseRate").val())) {
                        showWarning("", "Purchase Rate must be upto 2 characters before decimal point and 3 characters after decimal point long.");
                        $("#PurchaseRate").focus();
                        return false;
                    }
                }

                if (($("#AmountCalcId").val() == null) || ($("#AmountCalcId").val() == "")) {
                    showWarning("", "Please select Calculation On.");
                    $("#AmountCalcId").focus();
                    return false;
                }

                if ($("#Cut").val() != null && $("#Cut").val() != "") {
                    if (!/^[-+]?(\d{1,2}(\.\d{0,2})?|\.\d{1,2})$/.test($("#Cut").val())) {
                        showWarning("", "Cut must be upto 2 characters before decimal point and 2 characters after decimal point long.");
                        $("#Cut").focus();
                        return false;
                    }
                }

                if ($("#RolMin").val() != null && $("#RolMin").val() != "") {
                    if (!/^[-+]?(\d{1,3}(\.\d{0,2})?|\.\d{1,2})$/.test($("#RolMin").val())) {
                        showWarning("", "Rol Min must be upto 3 characters before decimal point and 2 characters after decimal point long.");
                        $("#RolMin").focus();
                        return false;
                    }
                }

                if ($("#RolMax").val() != null && $("#RolMax").val() != "") {
                    if (!/^[-+]?(\d{1,5}(\.\d{0,2})?|\.\d{1,2})$/.test($("#RolMax").val())) {
                        showWarning("", "Rol Max must be upto 5 characters before decimal point and 2 characters after decimal point long.");
                        $("#RolMax").focus();
                        return false;
                    }
                }

                if ($("#DenierWeight").val() != null && $("#DenierWeight").val() != "") {
                    if (!/^[-+]?(\d{1,2}(\.\d{0,3})?|\.\d{1,3})$/.test($("#DenierWeight").val())) {
                        showWarning("", "Yard Denier / Wt. must be upto 2 characters before decimal point and 3 characters after decimal point long.");
                        $("#DenierWeight").focus();
                        return false;
                    }
                }

                if ($("#RatePerMeter").val() != null && $("#RatePerMeter").val() != "") {
                    if (!/^[-+]?(\d{1,2}(\.\d{0,3})?|\.\d{1,3})$/.test($("#RatePerMeter").val())) {
                        showWarning("", "Rate / Meter must be upto 2 characters before decimal point and 3 characters after decimal point long.");
                        $("#RatePerMeter").focus();
                        return false;
                    }
                }

                if ($("#ProductBranchId").val() == "") {
                    showWarning("", "Please select branch.");
                    $("#ProductBranchId").focus();
                    return false;
                }

                var savedata = new Object();

                savedata.Id = parseInt($("#Id").val());

                savedata.Title = $("#Title").val();
                savedata.Code = $("#Code").val();
                savedata.PrintName = $("#PrintName").val();
                savedata.TitleAlternate = $("#TitleAlternate").val();

                savedata.DesignNumberId = 0;
                savedata.PackingId = 0;
                savedata.ShadeNumberId = 0;
                savedata.FabricId = 0;
                savedata.GroupId = 0;
                savedata.SubGroupId = 0;
                savedata.QualityTypeId = 0;
                savedata.CategoryId = 0;
                savedata.StockTypeId = 0;
                savedata.UQCId = 0;
                savedata.GSTCalculationId = 0;
                savedata.GSTRateId = 0;
                savedata.AmountCalcId = $("#AmountCalcId").val();

                savedata.ProductDesignNumber = $("#DesignNumberId option:selected").text();
                savedata.ColorName = $("#ColourName option:selected").text();
                savedata.SizeName = $("#SizeName option:selected").text();
                savedata.PackingName = $("#PackingId option:selected").text();
                savedata.ShadeNumber = $("#ShadeNumberId option:selected").text();
                savedata.FabricName = $("#FabricId option:selected").text();
                savedata.GroupName = $("#GroupId option:selected").text();
                savedata.SubGroupName = $("#SubGroupId option:selected").text();
                savedata.StockTypeName = $("#StockTypeId option:selected").text();
                savedata.QualityTypeName = $("#QualityTypeId option:selected").text();
                savedata.UQCTypeName = $("#UQCId option:selected").text();
                savedata.CategoryName = $("#CategoryId option:selected").text();
                savedata.GSTCalculationName = $("#GSTCalculationId option:selected").text();
                savedata.GSTRateName = $("#GSTRateId option:selected").text();
                savedata.GstCentralCess = $("#GstCentralCess option:selected").text();

                savedata.HSNCode = $("#HSNCode").val();

                savedata.DenierWeight = getFloatValueNull("DenierWeight", 3);
                savedata.RatePerMeter = getFloatValueNull("RatePerMeter", 3);
                savedata.RateRetail = getFloatValueNull("RateRetail", 3);
                savedata.Mrp = getFloatValueNull("Mrp", 3);
                savedata.DistributorRate = getFloatValueNull("DistributorRate", 3);
                savedata.DealerRate = getFloatValueNull("DealerRate", 3);
                savedata.PurchaseRate = getFloatValueNull("PurchaseRate", 3);
                savedata.Cut = getFloatValueNull("Cut", 2);
                savedata.AveragePack = getFloatValueNull("AveragePack", 2);
                savedata.BoxPack = getFloatValueNull("BoxPack", 2);
                savedata.RolMin = getFloatValueNull("RolMin", 2);
                savedata.RolMax = getFloatValueNull("RolMax", 2);
                savedata.GstStateCess = getFloatValueZero("GstStateCess", 2);
                savedata.Discount = getFloatValueZero("Discount", 2);

                savedata.Remarks1 = $("#Remarks1").val();
                savedata.Remarks2 = $("#Remarks2").val();
                savedata.HSNDesc = $("#HSNDesc").val();
                savedata.RateUpdate = $("#RateUpdate").is(':checked');
                savedata.Active = $('#Active').is(':checked');
                savedata.ProductBranchId = $("#ProductBranchId").val().join(",");

                blockUI('container');

                $.ajax({
                    type: "POST",
                    url: "@(Url.Action("SaveProduct", "Customer"))",
                    data: JSON.stringify(savedata),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {
                        unblockUI('container');
                        if (data.result == RESULT_SUCCESS) {
                            fromReset();
                            showWarning("", 'Product saved successfully.');
                            $("#Id").val(0);
                            refreshDatatableList();
                            $('.btn-back').click();
                        } else {
                            showError("", data.data);
                        }
                    },
                    error: function (error) {
                        unblockUI('container');
                        showError("", error.statusText);
                    }
                });

            }
            catch (e) {
                unblockUI('container');
                showError("", e.message);
            }
        }

