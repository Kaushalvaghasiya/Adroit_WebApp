@section Scripts
{
    <script>

        $(function (e) {

            $('.dv-list').show();
            $('.dv-detail').hide();

            $("#Title").on('change', function (e) {
                $("#PrintName").val($("#Title").val());
            });

            $("#GroupId").on('change', function (e) {
                ChangeProductValue();
            });

            $("#DesignNumberId").on('change', function (e) {
                ChangeProductValue();
            });

            $("#FabricId").on('change', function (e) {
                ChangeProductValue();
            });

            $("#SizeName").on('change', function (e) {
                ChangeProductValue();
            });

            $("#ColourName").on('change', function (e) {
                ChangeProductValue();
            });

            $("#SubGroupId").on('change', function (e) {
                ChangeProductValue();
            });

            $("#ShadeNumberId").on('change', function (e) {
                ChangeProductValue();
            });

            $("#GSTRateId").on('change', function (e) {

                var spanElement = document.getElementById("select2-GSTRateId-container");
                var selectedValue = spanElement.getAttribute("title");

                $("#CGST").val(selectedValue / 2);
                $("#SGST").val(selectedValue / 2);

            });

            bindDatatableList();


        });

        function ChangeProductValue() {

            var Tmp_TitleAlternate = '';

            var GroupElement = document.getElementById("select2-GroupId-container");
            var selectedGroup = GroupElement.getAttribute("title");

            var DesignNumberElement = document.getElementById("select2-DesignNumberId-container");
            var selectedDesignNumber = DesignNumberElement.getAttribute("title");

            var FabricElement = document.getElementById("select2-FabricId-container");
            var selectedFabric = FabricElement.getAttribute("title");

            var SizeElement = document.getElementById("select2-SizeName-container");
            var selectedSize = SizeElement.getAttribute("title");

            var ColourElement = document.getElementById("select2-ColourName-container");
            var selectedColour = ColourElement.getAttribute("title");

            var SubGroupElement = document.getElementById("select2-SubGroupId-container");
            var selectedSubGroup = SubGroupElement.getAttribute("title");

            var ShadeNumberElement = document.getElementById("select2-ShadeNumberId-container");
            var selectedShadeNumber = ShadeNumberElement.getAttribute("title");

            if (selectedGroup != null) {
                Tmp_TitleAlternate += (selectedGroup);
            }
            if (selectedDesignNumber != null) {
                Tmp_TitleAlternate += (" | " + selectedDesignNumber);
            }
            if (selectedFabric != null) {
                Tmp_TitleAlternate += (" | " + selectedFabric);
            }
            if (selectedSize != null) {
                Tmp_TitleAlternate += (" | " + selectedSize);
            }
            if (selectedColour != null) {
                Tmp_TitleAlternate += (" | " + selectedColour);
            }
            if (selectedSubGroup != null) {
                Tmp_TitleAlternate += (" | " + selectedSubGroup);
            }
            if (selectedShadeNumber != null) {
                Tmp_TitleAlternate += (" | " + selectedShadeNumber);
            }

            let res = Tmp_TitleAlternate;
            if (res.startsWith(" | ")) {
                res = res.slice(3);
            }

            $("#TitleAlternate").val(res);

        }

        function addClick() {
            $('.dv-list').slideUp();
            $('.dv-detail').slideDown();
            fromReset();
            setScreenMode(false);
            $(".btn-delete").hide();
            $("#Title").focus();
            $("#RateUpdate").prop('checked', true);
            $("#Active").prop('checked', true);
        }

        function viewMode(id) {
            getDetail(id, true);
        }

        function editMode(id) {
            getDetail(id, false);
        }

        function refreshDatatableList() {
            $('#tblMain').DataTable().ajax.reload();
        }

        function bindDatatableList() {
            let columns = [];
            let colDef = [];
            colDef.push({
                "render": function (data, type, row) {
                    return row.code;
                },
                "targets": colDef.length,
            });
            colDef.push({
                "render": function (data, type, row) {
                    return row.title;
                },
                "targets": colDef.length,
            });
            colDef.push({
                "render": function (data, type, row) {
                    return row.productDesignNumber;
                },
                "targets": colDef.length,
            });
            colDef.push({
                "render": function (data, type, row) {
                    return row.colorName;
                },
                "targets": colDef.length,
            });
            colDef.push({
                "render": function (data, type, row) {
                    return row.uqcTypeName;
                },
                "targets": colDef.length,
            });
            colDef.push({
                "render": function (data, type, row) {
                    return row.gstCalculationName;
                },
                "targets": colDef.length,
            });
            colDef.push({
                "render": function (data, type, row) {
                    let btnAct = "<div class='btn-list'> ";
                    btnAct += "<a href='#' class='btn-edit btn btn-outline-warning small-icon-btn' data-value='" + row.id + "' data-toggle='tooltip' title='Edit' data-placement='bottom' data-original-title='Edit'><i class='fa fa-pencil'></i></a>&nbsp;&nbsp;";
                    btnAct += "<a href='#' class='btn-view btn btn-outline-info small-icon-btn' data-value='" + row.id + "' data-toggle='tooltip' title='View' data-placement='bottom' data-original-title='View'><i class='fa fa-info'></i></a>&nbsp;&nbsp;";
                    btnAct += "<a href='#' class='btn-delete-grid btn btn-outline-danger small-icon-btn' data-value='" + row.id + "' data-toggle='tooltip' title='Delete' data-placement='bottom' data-original-title='Delete'><i class='fa fa-trash-o'></i></a>";
                    btnAct += "</div>";
                    return btnAct;
                },
                "targets": colDef.length,
                "className": "text-center",
                "orderable": false
            });

            columns.push({ "data": "code", "name": "code" });
            columns.push({ "data": "title", "name": "title" });
            columns.push({ "data": "productDesignNumber", "name": "productDesignNumber" });
            columns.push({ "data": "colorName", "name": "colorName" });
            columns.push({ "data": "uqcTypeName", "name": "uqcTypeName" });
            columns.push({ "data": "gstCalculationName", "name": "gstCalculationName" });

            var table = $('#tblMain').DataTable({
                "bAutoWidth": false,
                lengthChange: false,
                ordering: true,
                searching: true,
                paging: true,
                pageLength: 50,
                order: [[0, 'asc']],
                pagingType: 'full_numbers',
                orderClasses: false,
                //scrollCollapse: true,
                //stateSave: false,
                columnDefs: colDef,
                columns: columns,
                language: {
                    paginate: {
                        next: '<i class="fa fa-angle-right"></i>', // or '→'
                        previous: '<i class="fa fa-angle-left"></i>', // or '←'
                        first: '<i class="fa fa-angle-double-left"></i>', // or '→'
                        last: '<i class="fa fa-angle-double-right"></i>' // or '←'
                    }
                },
                buttons: ['copy', 'excel', 'colvis'],
                processing: true,
                serverSide: true,
                ajax: {
                    url: '@(Url.Content("~/Customer/ProductList"))',
                    type: 'GET',
                    data: function (d) { }
                },
                "initComplete": function (settings, json) {
                    gridInitComplete(table);
                },
                "drawCallback": function (settings) {
                    gridDrawCallback(this);
                }

            });
        }

