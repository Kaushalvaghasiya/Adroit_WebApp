            $("#ColourName").val(null).trigger('change');
            $("#SizeName").val(null).trigger('change');
            $("#PackingId").val(null).trigger('change');
            $("#ShadeNumberId").val(null).trigger('change');
            $("#FabricId").val(null).trigger('change');
            $("#GroupId").val(null).trigger('change');
