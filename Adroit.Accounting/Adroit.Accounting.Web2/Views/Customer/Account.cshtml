        function SaveDetail() {
            try {
                //if ($("#CustomerId").length > 0 && $("#CustomerId").val() == "") {
                //    showWarning("", "Customer not found.");
                //    return;
                //}
                if ($("#Name").length > 0 && $("#Name").val() == "") {
                    showWarning("", "Please enter name.");
                    $("#Name").focus();
                    return;
                }
                if ($("#AccountGroupId").length > 0 && $("#AccountGroupId").val() == "") {
                    showWarning("", "Please select Account Group.");
                    $("#AccountGroupId").focus();
                    return;
                }                
                if ($("#GSTInvoiceTypeId").length > 0 && $("#GSTInvoiceTypeId").val() == "") {
                    showWarning("", "Please select GST Invoice Type.");
                    $("#GSTInvoiceTypeId").focus();
                    return;
                }

                let isValidemail = isEmail($("#Email").val());
                if (!isValidemail) {
                    showWarning("", "Please enter valid email");
                    return;
                }

                var savedata = new Object;
                savedata.Id = parseInt($("#Id").val());
                savedata.CustomerId = parseInt($("#CustomerId").val());
                savedata.Name = $("#Name").val();
                savedata.PrintName = $("#PrintName").val();
                savedata.AccountGroupId = parseInt($("#AccountGroupId").val());
                savedata.Address1 = $("#Address1").val();
                savedata.Address2 = $("#Address2").val();
                savedata.Address3 = $("#Address3").val();
                savedata.CountryId = $("#CountryId").val() == "" ? null : parseInt($("#CountryId").val());
                savedata.StateId = $("#StateId").val() == "" ? null : parseInt($("#StateId").val());
                savedata.CityId = $("#CityId").val() == "" ? null : parseInt($("#CityId").val());
                savedata.Pincode = $("#Pincode").val();
                savedata.KM = $("#KM").val();
                savedata.AreaName = $("#AreaName option:selected").text();
                savedata.ContactPersonName = $("#ContactPersonName").val();
                savedata.Mobile = $("#Mobile").val();
                savedata.MobileAlternate = $("#MobileAlternate").val();
                savedata.Email = $("#Email").val();
                savedata.AdharUID = $("#AdharUID").val();
                savedata.GSTNumber = $("#GSTNumber").val();
                savedata.PAN = $("#PAN").val();
                savedata.TAN = $("#TAN").val();
                savedata.EximCode = $("#EximCode").val();
                savedata.GSTInvoiceTypeId = $("#GSTInvoiceTypeId").val();
                savedata.CreditDays = $("#CreditDays").val() == "" ? null : parseInt($("#CreditDays").val());
                savedata.CreditLimit = $("#CreditLimit").val() == "" ? null : parseFloat($("#CreditLimit").val());
                savedata.TransportName = $("#TransportName option:selected").text();
                savedata.GSTNumberTransport = $("#GSTNumberTransport").val();
                savedata.ShippingAccountBranchMappingId = $("#ShippingAccountBranchMappingId").val() == "" ? null : parseInt($("#ShippingAccountBranchMappingId").val());
                savedata.DeliveryAccountBranchMappingId = $("#DeliveryAccountBranchMappingId").val() == "" ? null : parseInt($("#DeliveryAccountBranchMappingId").val());
                savedata.BrokerMappingId = $("#BrokerMappingId").val() == "" ? null : parseInt($("#BrokerMappingId").val());
                savedata.TDS = $("#TDS").val() == "" ? null : parseFloat($("#TDS").val());
                savedata.TCS = $("#TCS").val() == "" ? null : parseFloat($("#TCS").val());
                savedata.Discount = $("#Discount").val() == "" ? null : parseFloat($("#Discount").val());
                savedata.InterestRate = $("#InterestRate").val() == "" ? null : parseFloat($("#InterestRate").val());
                savedata.Commission = $("#Commission").val() == "" ? null : parseFloat($("#Commission").val())
                savedata.CapitalPercentage = $("#CapitalPercentage").val() == "" ? null : parseFloat($("#CapitalPercentage").val());
                savedata.IsIGST = $("#IsIGST").prop("checked");
                savedata.RateWithGST = $("#RateWithGST").prop("checked");
                savedata.RCMParty = $("#RCMParty").prop("checked");
                savedata.CompositParty = $("#CompositParty").prop("checked");
                savedata.IsEcommerce = $("#IsEcommerce").prop("checked");
                savedata.IsActive = $("#IsActive").prop("checked");
                savedata.IsDeleted = false;
                var jsonData = JSON.stringify(savedata);
                console.log(jsonData);
                blockUI('container');
                var retrundata = AjaxCall("POST", "@(Url.Action("SaveAccount", "Customer"))", jsonData,
                    function (data) {
                        unblockUI('container');
                        if (data.result == RESULT_SUCCESS) {
                            fnReset();
                            showWarning("", 'Account saved successfully.');
                            $("#Id").val(0);
                            datatablelist();
                        } else {
                            showError("", data.data);
                        }
                    },
                    function (error) {
                        unblockUI('container');
                        showError("", error.statusText);
                    });
            }
            catch (e) {
                unblockUI('container');
                showError("", e.message);
                //swal('Error', e.message, "error"); //warning,  success, error, info
            }
        }

