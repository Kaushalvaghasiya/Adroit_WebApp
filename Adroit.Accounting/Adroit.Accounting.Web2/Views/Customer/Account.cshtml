    <script src="@Url.Content("~/assets/plugins/spectrum-date-picker/jquery-ui.js")"></script>
    <script src="@Url.Content("~/assets/plugins/input-mask/jquery.maskedinput.js")"></script>
    <!-- Timepicker js -->
    <script src="@Url.Content("~/assets/plugins/time-picker/jquery.timepicker.js")"></script>
    <script src="@Url.Content("~/assets/plugins/time-picker/toggles.min.js")"></script>

    <script>
        var RESULT_SUCCESS = "@Adroit.Accounting.Utility.Constant.API_RESULT_SUCCESS";

        $(function (e) {
            $('.dv-list').show();
            $('.dv-detail').hide();

            $('.btn-add-new').click(function () {
                $('.dv-list').slideUp();
                $('.dv-detail').slideDown();
            });

            $('.btn-back').click(function () {
                $('.dv-detail').slideUp();
                $('.dv-list').slideDown();
            });
            datatablelist();

            $(".buttons-copy").removeClass("btn-primary");
            $(".buttons-copy").addClass("btn-outline-primary");

            $(".buttons-excel").removeClass("btn-primary");
            $(".buttons-excel").addClass("btn-outline-primary");

            $(".buttons-pdf").removeClass("btn-primary");
            $(".buttons-pdf").addClass("btn-outline-primary");

            $(".buttons-colvis").removeClass("btn-primary");
            $(".buttons-colvis").removeClass("dropdown-toggle");
            $(".buttons-colvis").addClass("btn-outline-primary sub-icon");

            $(".buttons-copy").empty();
            $(".buttons-copy").append('<i class="fa fa-copy" data-toggle="tooltip" title="Copy" data-original-title="fa fa-copy"></i>');

            $(".buttons-excel").empty();
            $(".buttons-excel").append('<i class="fa fa-file-excel-o" data-toggle="tooltip" title="Excel" data-original-title="fa fa-file-excel-o"></i>');

            $(".buttons-pdf").empty();
            $(".buttons-pdf").append('<i class="fa fa-file-pdf-o" data-toggle="tooltip" title="PDF" data-original-title="fa fa-file-pdf-oo"></i>');

            $(".buttons-colvis").empty();
            $(".buttons-colvis").append('<i class="fa fa-eye" data-toggle="tooltip" title="Column Visibility" data-original-title="fa fa-eye"></i>');

            BindCountryList();
            BindGSTInvoiceTypeList();
            BindStateList($("#CountryId").val());
            $("#CountryId").on('change', function (e) {
                let countryId = $(this).val();
                if (countryId > 0) {
                    $("#CountryCallingCode").val(countryId);
                    BindStateList(countryId);
                }
                else {
                    $("#CountryId").focus();
                    showWarning("", "Please select country.");
                }
            });
            $("#StateId").on('change', function (e) {
                let stateId = $(this).val();
                if (stateId > 0) {
                    BindDistrictList(stateId);
                } else {
                    $("#StateId").focus();
                    showWarning("", "Please select state.");
                }
            });
            $("#DistrictId").on('change', function (e) {
                let districtId = $(this).val();
                if (districtId > 0) {
                    BindTalukaList(districtId);
                } else {
                    $("#DistrictId").focus();
                    showWarning("", "Please select district.");
                }
            });
            $("#TalukaId").on('change', function (e) {
                let talukaId = $(this).val();
                if (talukaId > 0) {
                    BindCityList(0, talukaId, 0);
                } else {
                    $("#TalukaId").focus();
                    showWarning("", "Please select taluka.");
                }
            });
            BindCustomerAccountGroupList();
            BindCustomerBrokerBranchMappingList();
            BindCustomerAccountList();
        });

